{
  "name": "screen-recorder",
  "version": "2019.2.20",
  "description": "Free screen recorder runs on all platforms",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "packInstaller": "electron-builder"
  },
  "build": {
    "asar": false,
    "appId": "top.yunp.screenrecorder",
    "productName": "ScreenRecorder",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "src/res/icons/ScreenRecorder.ico"
    },
    "mac": {
      "icon": "src/res/icons/ScreenRecorder.icns"
    }
  },
  "author": "TOPYUNP",
  "devDependencies": {
    "electron": "^9.1.1",
    "electron-builder": "^20.38.5",
    "gulp": "^4.0.0",
    "js2wc": "^1.0.11",
    "shelljs": "^0.8.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "vue": "^2.5.22"
  }
}
